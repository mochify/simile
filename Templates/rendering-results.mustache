<h1>{{reportname}}</h1>

<div>
    <h2>Failures</h2>
    <p>{{failures}}</p>
</div>

<div>
    <h2>Results</h2>
    <table>
    <tr>
        <td>Test Case</td>
        <td>Passed</td>
        <td>Reference Image</td>
        <td>Preview Image</td>
        <td>Difference</td>
    </tr>
    {{#result}}
        <tr>
            <td>{{TestId}}</td>
            <td>{{TestPassed}}</td>
            <td><img src='{{Reference}}'</td>
            <td><img src='{{Preview}}'</td>
            <td><img src='{{Difference}}'</td>
        </tr>
    {{/result}}
    </table>
</div>